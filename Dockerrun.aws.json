{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "eleach6633/multi-client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "server",
      "image": "eleach6633/multi-server",
      "hostname": "api",
      "essential": false

    },
    {
    "name": "worker",
    "image": "eleach6633/multi-worker",
    "hostname": "worker",
    "essential": false

    },
    {
      "name": "nginx",
      "image": "eleach6633/multi-worker",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [ "client", "service"]
    }
  ]
}